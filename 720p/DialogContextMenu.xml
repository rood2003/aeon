<window id="106">
	<defaultcontrol>1000</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="144,214" easing="out" tween="back" time="300" />
		<effect type="fade" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="144,214" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	<controls> 

		<control type="image" id="997">
			<description>background top image</description>
			<posx>0</posx>
			<posy>-26</posy>
			<width>308</width>
			<height>26</height>
			<include condition="!Skin.HasSetting(darkshowcase)">ContextTopLight</include>
			<include condition="Skin.HasSetting(darkshowcase)">ContextTopNight</include>
		</control>
		<control type="image" id="999">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>308</width>
			<include condition="!Skin.HasSetting(darkshowcase)">ContextMiddleLight</include>
			<include condition="Skin.HasSetting(darkshowcase)">ContextMiddleNight</include>
		</control>
		<control type="image" id="998">
			<description>background bottom image</description>
			<posx>0</posx>
			<posy>390</posy>
			<width>308</width>
			<height>27</height>
			<include condition="!Skin.HasSetting(darkshowcase)">ContextBottomLight</include>
			<include condition="Skin.HasSetting(darkshowcase)">ContextBottomNight</include>
		</control>
		<control type="button" id="1000">
			<description>button template</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>39</height>
			<width>308</width>
			<include condition="!Skin.HasSetting(darkshowcase)">ContextFocusLight</include>
			<include condition="Skin.HasSetting(darkshowcase)">ContextFocusNight</include>
			<align>center</align>
			<aligny>top</aligny>
			<font>Font_ContextMenuButton</font>
			<textoffsety>8</textoffsety>
		</control>

	</controls>
</window>